{
  "expo": {
    "name": "Busmate",
    "slug": "busmate-passenger-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/busmate-icon-inverted.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d39e07da-7ac8-472f-98b9-655719023213"
      }
    },
    "owner": "kavindadimuthu",
    "splash": {
      "image": "./assets/images/busmate-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#004CFF"
    },
    "android": {
      "package": "com.kavindadimuthu.busmatepassengerapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/busmate-round-icon.png",
        "backgroundColor": "#004CFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "statusBar": {
        "backgroundColor": "#004CFF",
        "barStyle": "light-content",
        "translucent": false
      }
    }
  }
}
